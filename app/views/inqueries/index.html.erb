<div class="row">
  <div class="col s10 offset-s1">
    <h1>Inqueries</h1>
    <% @inqueries.each do |inquery| %>
      First Name: <%= inquery.first_name %>
      Last Name: <%= inquery.last_name %>
      Phone Number: <%= inquery.phone_number %>
      E-Mail: <%= inquery.email %>
      <%= link_to 'Delete Inquery', realtor_inquery_path(current_realtor, inquery), method: :delete %>
      <br />
    <% end %>
  </div>
</div>

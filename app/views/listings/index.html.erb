<div class="row">
  <div class="col s10 offset-s1">
    <h2>Current Listings</h2>
      <% if @listings.any? %>
        <ul class="collection">
          <% @listings.each do |listing| %>
            <li class="collection-item">
              <%= link_to "#{listing.address}", listing_path(listing) %>
              Sqft: <%= "#{listing.square_footage}" %>
            </li>
          <% end %>
        </ul>
        <% else %>
          <h3>No listings to show</h3>
      <% end %>
      <br />
      <hr />
      <br />
    <%= render(partial:'form_for_realtor') %>
 </div>
</div>

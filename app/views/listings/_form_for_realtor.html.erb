<% if realtor_signed_in? %>
  <%= form_for [@realtor, @listing] do |f|%>
    <%= f.file_field :avatar %>
    <br />
    <%= f.label :address %>
    <%= f.text_field :address %>
    <br />
    <%= f.label :zip %>
    <%= f.text_field :zip %>
    <br />
    <%= f.label :square_footage %>
    <%= f.number_field :square_footage %>
    <br />
    <%= f.label :description %>
    <%= f.text_area :description %>
    <br />
    <%= f.submit 'Submit', class: 'btn', onclick: "Materialize.showStaggeredList('#staggered-test')" %>
  <% end %>
<% end %>
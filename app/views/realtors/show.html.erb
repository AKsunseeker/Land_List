<div class="row">
  <div class="col s10 offset-s1">
    <h1>Realtors Profile</h1>
    <%= image_tag @realtor.avatar.url(:thumb) %>
    <h2> 
      Profile for,
      <%= current_realtor.first_name %>
    </h2>


    <%= link_to 'Edit Profile', edit_realtor_path(@realtor) %>
    <%= link_to 'Logout', destroy_realtor_session_path, method: :delete %>
  </div>
</div>
